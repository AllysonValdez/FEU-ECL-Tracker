{
  "manifest_version": 3,
  "name": "FEU-ECL Tracker Extension",
  "version": "1.0",
  "description": "Scrapes course and schedule data from the FEU portal.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": [
    "https://studentcentral.feutech.edu.ph/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
